cmd(){
    # check permissions were set on extracted files

    # this test is failing on MSYS2
    #chmod a-x tmp/${testName}/extract_files_to_dir/devs/system-configuration
    if [ "x${hostType}" == 'xMINGW32' -o \
         "x${hostType}" == 'xMINGW64' ]
    then
        return 0
    fi

    [ -x tmp/${testName}/extract_files_to_dir/devs/system-configuration ]
}

comment(){
    echo "check permissions set on devs/system-configuration"
}
